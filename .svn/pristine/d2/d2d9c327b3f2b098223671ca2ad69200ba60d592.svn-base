        public static List<FT0003ACUC> ListarFacturas(FT0003ACUC ADATA)
        {
            using (var ctx=new RSFACAR())
            {
                return (from c in ctx.FT0003ACUC where c.F5_CNUMSER.Trim() == ADATA.F5_CNUMSER.Trim() && c.F5_CTD== ADATA.F5_CTD && c.F5_CNUMDOC.Contains(ADATA.F5_CNUMDOC) && c.F5_CCODCLI.Trim()==ADATA.F5_CCODCLI.Trim() select c).ToList();
            }
        }

        public static List<FT0003ACUC> ListaFacturasEs(string fin, string ffn, string estado,string tipodoc)
        {
            List<FT0003ACUC> resp = new List<FT0003ACUC>();
            using (RSFACAR context = new RSFACAR())
            {
                SqlParameter p_f1 = new SqlParameter("fin", fin);
                SqlParameter p_f2 = new SqlParameter("ffn", ffn);
                SqlParameter p_estado = new SqlParameter("estado", estado);
                SqlParameter p_tipodoc = new SqlParameter("tipodoc", tipodoc);
                //var consul = "exec PR_KARDEX_PERIODO @fin ='" + finicio + "',@ffn='" + ffina + "',@codigo='" + codigo + "' ";
                //datos = ctx.VISTA_KARDEVAL.SqlQuery(consul).ToList();

                resp = context.FT0003ACUC.SqlQuery("exec dbo.PR_FACTURAS_SCP @fin,@ffn,@estado,@tipodoc", p_f1, p_f2, p_estado, p_tipodoc).ToList();
                return resp;
            }
        }




